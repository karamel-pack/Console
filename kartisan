<?php
spl_autoload_register(function ($class) {
    $class = str_replace("Karamel\\Console\\", "", $class);
    $class = str_replace("\\", "/", $class);
    require_once __DIR__ . '/src/' . $class . '.php';
});

\Karamel\Console\Commands\Boot::start();

